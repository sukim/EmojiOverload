{
  "name": "Emojify",
  "description": "Emoji overload",
  "version": "1.0",
  "permissions": [
    "tabs", "http://*/*", "https://*/*"
  ],
  "browser_action": {
      "default_title": "Set this page's color.",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,

    "web_accessible_resources": [
        "popup.js",
        "manifest.json",
        "emoji/*",
        "meow/*",
        "bear/*",
        "moon/*",
        "doraemon/*"

    ]
}